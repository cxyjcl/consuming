<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.whpu.management.dao.CharacterDao">
    <select id="getCharacterId" parameterType="String" resultType="Integer">
        select id from t_character where charactername=#{name};
    </select>
</mapper>
